{
  "Framework": "jasmine",
  "EnableCodeCoverage": "true",
  "CodeCoverageExcludes": [
    "*\\vendor\\*"
  ],
  "CodeCoverageIgnores": [
    "*\\Test\\*"
  ],
  "References": [
    { "Path": "vendor/jquery-2.1.4.js" },
    {
      "Path": "Scripts/jasmine",
      "Include": "*.js"
    }
  ],
  "Tests": [
    {
      "Path": "Scripts/jasmine-samples",
      "Include": "*.js"
    }
  ]
}