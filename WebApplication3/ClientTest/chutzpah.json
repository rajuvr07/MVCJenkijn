{
  "Framework": "jasmine",
  "EnableCodeCoverage": "true",
  "CodeCoverageExecutionMode": "Always",

  "CodeCoverageIncludes": [
    "*\\test\\*"
  ],
  "Transforms": [
    {
      "Name": "jacoco",
      "Path": "ChutzpahJacoco.xml"
    },
    {
      "Name": "lcov",
      "Path": "ChutzpahJS.lcov"
    },
    {
      "Name": "coveragehtml",
      "Path": "ChutzpahJS.html"
    }
  ],
  "References": [
    {
      "Path": "../webapplication3/scripts/jquery-3.3.1.min.js"
    },
    {
      "Path": "../webapplication3/views/",
      "Include": "*.js"
    }
  ],
  "Tests": [
    {
      "Path": "test",
      "Include": "*.js"
    }
  ]
}